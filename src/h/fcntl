#ifndef LIBOS_FCNTL_H
#define LIBOS_FCNTL_H

#ifdef __CC_NORCROFT
#pragma include_only_once
#endif

#include "common.h"

// Access modes
#define O_RDONLY 0
#define O_WRONLY 1
#define O_RDWR   2
#define O_ACCMODE 3

// Creation flags
#define O_CREAT  0x0100
#define O_EXCL   0x0200
#define O_TRUNC  0x0400
#define O_APPEND 0x0800

#endif // LIBOS_FCNTL_H
